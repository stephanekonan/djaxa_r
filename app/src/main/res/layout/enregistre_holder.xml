<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="85dp"    android:layout_marginBottom="4dp"    xmlns:app="http://schemas.android.com/apk/res-auto">    <TextView        android:id="@+id/customerName"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/phoneCustomer"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/description"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/phoneModel"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        android:textColor="@color/black_primary"        android:layout_marginStart="16dp"        android:layout_marginTop="8dp"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/issue"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="180 000"        android:fontFamily="@font/poppinsbold"        android:textColor="@color/black_primary"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintTop_toBottomOf="@id/phoneModel"        app:layout_constraintStart_toStartOf="@id/phoneModel"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/repairDate"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="F CFA"        android:fontFamily="@font/poppinsregular"        android:textColor="@color/black_primary"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintTop_toBottomOf="@id/issue"        app:layout_constraintStart_toStartOf="@id/issue"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/status"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        android:layout_marginEnd="16dp"        android:layout_marginTop="10dp"        android:text="enregistre"        tools:ignore="HardcodedText" /></androidx.constraintlayout.widget.ConstraintLayout>