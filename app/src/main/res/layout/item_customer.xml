<?xml version="1.0" encoding="utf-8"?><androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="85dp"    xmlns:app="http://schemas.android.com/apk/res-auto">    <TextView        android:id="@+id/repairId"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/customerName"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/descriptionRepair"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/phoneCustomer"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/montantNormal"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/pieceId"        android:visibility="gone"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Achat de téléphone Iphone 11"        android:fontFamily="@font/poppinsbold"        tools:ignore="HardcodedText,MissingConstraints" />    <TextView        android:id="@+id/phoneModel"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Samsung M11"        android:fontFamily="@font/poppinsregular"        android:textColor="@color/black_primary"        android:layout_marginTop="8dp"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintTop_toTopOf="parent"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/issue"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="Ecran cassé, circuit de charge"        android:fontFamily="@font/poppinsregular"        android:textColor="@color/black_primary"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintTop_toBottomOf="@id/phoneModel"        app:layout_constraintStart_toStartOf="@id/phoneModel"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/dateDeposit"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:text="dsfdgfhgj"        android:fontFamily="@font/poppinsregular"        android:textColor="@color/gray_text"        app:layout_constraintHorizontal_bias="0.0"        app:layout_constraintTop_toBottomOf="@id/issue"        app:layout_constraintStart_toStartOf="@id/issue"        tools:ignore="HardcodedText" />    <TextView        android:id="@+id/montantNegocie"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_marginTop="8dp"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toTopOf="parent"        android:text="35000 XOF"        android:fontFamily="@font/poppinssemibold"        android:textColor="@color/black_primary"        tools:ignore="ContentDescription,HardcodedText" />    <TextView        android:id="@+id/status"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintTop_toBottomOf="@+id/montantNegocie"        android:text="enregistré"        tools:ignore="ContentDescription,HardcodedText" />    <View        android:layout_width="match_parent"        android:layout_height="1dp"        android:background="#DBE1E0"        android:layout_marginTop="15dp"        app:layout_constraintStart_toStartOf="parent"        app:layout_constraintEnd_toEndOf="parent"        app:layout_constraintBottom_toBottomOf="parent" /></androidx.constraintlayout.widget.ConstraintLayout>